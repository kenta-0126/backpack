<h1>イベント追加ページ</h1>
<%= form_with model: @event, url: events_path, local: true do |f| %>

<div>
  <table>

    <tr>
      <th>イベント追加</th>
      <td><%= f.text_field :title %></td>
    </tr>

    <tr>
      <th>予定日</th>
      <td><%= f.datetime_select :start_time %></td>
    </tr>

    <tr>
      <th></th>
      <td><%= f.submit "新規登録" %></td>
    </tr>

  </table>
</div>

<% end %>


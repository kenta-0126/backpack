<h1>ジャンル追加ページ</h1>
<div>
    <% form_with model: @genre, url: new_genre_path, local: true do |f| %>
    <%= f.text_field :name, placeholder: "カバンを追加" %>
    <%= f.submit "登録" %>
    <% end %>
  </div>

  <table>
    <thead>
      <tr>
        <th>持ってるかばん</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% @genres.each do |genre| %>
      <tr>
        <td><%= genre.name %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <div>
    
  </div>